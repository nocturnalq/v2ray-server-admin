{
  "log": {
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log",
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "port": {{ inbounds.port }},
      "protocol": "{{ inbounds.protocol }}",
      "settings": {
        "clients": [
          {% for client in inbounds.settings.clients %}
          {
            "id": "{{ client.id }}",
            "alterId": "{{ client.alterId }}",
            "security": "auto"
          }{{ "," if not loop.last else "" }}
          {% endfor %}
        ]
      },
      // "streamSettings": {
      //   "network": "ws",
      //   "wsSettings": {
      //     "path": "/"
      //   }
      // },
      // "mux": {
      //   "enabled": true
      // }
    }
  ],
  "outbounds": [
    {
      "protocol": "{{ outbounds.protocol }}",
      // "tag": "{{ outbounds.protocol }}",
      "settings": {}
    }
  ]
  "dns": {
    "servers": [
      "pihole-server",
      "8.8.8.8",
      "8.8.4.4",
      "localhost"
    ]
  }
}